{
  "expo": {
    "name": "Weatherman",
    "slug": "Weatherman",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/android-chrome-192x192-1.png",
    "sdkVersion": "39.0.0",
    "splash": {
      "image": "./assets/android-chrome-512x512.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "host.exp.exponent"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/android-chrome-512x512.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.pulsating_photon.weatherman"
    },
    "web": {
      "favicon": "./assets/favicon-16x16.png"
    }
  }
}
